{
    "name": "zerai/cf",
    "type": "project",
    "description": "POC application",
    "license": "proprietary",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        },
        {
            "name": "Francesca Bonadonna",
            "email": "francescabonadonna@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.1.*",
        "symfony/messenger": "6.1.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/phpunit-bridge": "^6.1"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Iam\\": "_iam/src/",
            "VehicleReporting\\": "_vehiclereporting/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Iam\\Tests\\": "_iam/tests/",
            "VehicleReporting\\Tests\\": "_vehiclereporting/tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test-app": "bin/phpunit",
        "test-iam": "bin/phpunit _iam/tests",
        "test-vrepo": "bin/phpunit _vehiclereporting/tests"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
